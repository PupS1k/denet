{
    "name": "ui",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build ui",
        "watch": "ng build ui --watch",
        "deploy": "ng lint && stylelint \"**/*.scss\" && ng build ui -c production",
        "test": "ng test --code-coverage --watch=false --source-map=false",
        "test-debug": "ng test --code-coverage --karmaConfig src/karma-debug.conf.js",
        "test-debug-no-coverage": "ng test --karmaConfig src/karma-debug.conf.js",
        "lint": "ng lint",
        "lint:fix": "ng lint --fix",
        "stylelint": "stylelint \"**/*.scss\"",
        "stylelint:fix": "stylelint \"**/*.scss\" --fix"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "~13.1.0",
        "@angular/common": "~13.1.0",
        "@angular/compiler": "~13.1.0",
        "@angular/core": "~13.1.0",
        "@angular/forms": "~13.1.0",
        "@angular/platform-browser": "~13.1.0",
        "@angular/platform-browser-dynamic": "~13.1.0",
        "@angular/router": "~13.1.0",
        "rxjs": "~7.4.0",
        "tslib": "^2.3.0",
        "zone.js": "~0.11.4"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~13.1.3",
        "@angular/cli": "~13.1.3",
        "@angular/compiler-cli": "~13.1.0",
        "@angular-eslint/builder": "~13.1.0",
        "@angular-eslint/eslint-plugin": "~13.1.0",
        "@angular-eslint/eslint-plugin-template": "~13.1.0",
        "@angular-eslint/schematics": "~13.1.0",
        "@angular-eslint/template-parser": "~13.1.0",
        "@types/jasmine": "~3.10.0",
        "@types/node": "^12.11.1",
        "@typescript-eslint/eslint-plugin": "4.28.2",
        "@typescript-eslint/parser": "4.28.2",
        "eslint": "^7.32.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-import-resolver-typescript": "^2.4.0",
        "eslint-plugin-angular": "^4.0.1",
        "eslint-plugin-html": "^6.1.2",
        "eslint-plugin-import": "^2.24.0",
        "eslint-plugin-jsdoc": "^36.0.7",
        "eslint-plugin-ordered-imports": "^0.5.0",
        "eslint-plugin-prefer-arrow": "^1.2.3",
        "eslint-plugin-prettier": "^3.4.0",
        "eslint-plugin-rxjs": "3.3.6",
        "eslint-plugin-rxjs-angular": "1.0.6",
        "jasmine-core": "~3.10.0",
        "karma": "~6.3.0",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage": "~2.1.0",
        "karma-jasmine": "~4.0.0",
        "karma-junit-reporter": "^1.2.0",
        "karma-jasmine-html-reporter": "~1.7.0",
        "karma-jasmine-seed-reporter": "^0.2.0",
        "prettier": "^2.3.2",
        "puppeteer": "4.0.0",
        "stylelint": "^13.13.1",
        "stylelint-scss": "^3.21.0",
        "typescript": "~4.5.2"
    }
}
